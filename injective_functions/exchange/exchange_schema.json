          "type": "object",
          "properties": {
              "price": {
                  "type": "string",
                  "description": "Limit price for the order"
              },
              "quantity": {
                  "type": "string",
                  "description": "Order quantity"
              },
              "side": {
                  "type": "string",
                  "enum": ["BUY", "SELL"],
                  "description": "Order side"
              },
              "market_id": {
                  "type": "string",
                  "description": "Derivatives market ID"
              },
              "subaccount_idx": {
                  "type": "integer",
                  "description": "Subaccount index for the order"
